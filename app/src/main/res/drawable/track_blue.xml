<?xml version="1.0" encoding="utf-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android"
    android:insetBottom="@dimen/track_vmargin"
    android:insetTop="@dimen/track_vmargin">
    <shape xmlns:android="http://schemas.android.com/apk/res/android"
        android:shape="rectangle">
        <size
            android:width="@dimen/track_width"
            android:height="@dimen/track_height" />

        <corners
            android:bottomLeftRadius="8dp"
            android:bottomRightRadius="8dp"
            android:topLeftRadius="8dp"
            android:topRightRadius="8dp" />

        <gradient
            android:endColor="#ff165B90"
            android:startColor="#ff165B90" />
        <stroke
            android:width="1dp"
            android:color="#ffc0c0c0" />

    </shape>
</inset>